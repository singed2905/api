{
  "latex_to_calculator_mappings": [
    {
      "find": "\\\\frac\\{([^{}]+)\\}\\{([^{}]+)\\}",
      "replace": "\\1a\\2",
      "type": "regex",
      "description": "Fraction conversion: \\frac{a}{b} -> aab",
      "priority": 1
    },
    {
      "find": "\\-",
      "replace": "p",
      "type": "regex",
      "description": "Negative sign: -x -> px",
      "priority": 2
    },
    {
      "find": "\\*",
      "replace": "O",
      "type": "regex",
      "description": "Multiplication: x*y -> xOy",
      "priority": 3
    },
    {
      "find": "\\/",
      "replace": "P",
      "type": "regex",
      "description": "Division: x/y -> xPy",
      "priority": 3
    },
    {
      "find": "\\\\sqrt\\{",
      "replace": "s",
      "type": "regex",
      "description": "Square root: \\sqrt{x} -> s(x)",
      "priority": 4
    },
    {
      "find": "sqrt\\{",
      "replace": "s",
      "type": "regex",
      "description": "Square root no backslash: sqrt{x} -> s(x)",
      "priority": 4
    },
    {
      "find": "\\\\sin\\(",
      "replace": "j(",
      "type": "regex",
      "description": "Sine function: \\sin(x) -> j(x)",
      "priority": 5
    },
    {
      "find": "sin\\(",
      "replace": "j(",
      "type": "regex",
      "description": "Sine function no backslash: sin(x) -> j(x)",
      "priority": 5
    },
    {
      "find": "\\\\cos\\(",
      "replace": "k(",
      "type": "regex",
      "description": "Cosine function: \\cos(x) -> k(x)",
      "priority": 5
    },
    {
      "find": "cos\\(",
      "replace": "k(",
      "type": "regex",
      "description": "Cosine function no backslash: cos(x) -> k(x)",
      "priority": 5
    },
    {
      "find": "\\\\tan\\(",
      "replace": "l(",
      "type": "regex",
      "description": "Tangent function: \\tan(x) -> l(x)",
      "priority": 5
    },
    {
      "find": "tan\\(",
      "replace": "l(",
      "type": "regex",
      "description": "Tangent function no backslash: tan(x) -> l(x)",
      "priority": 5
    },
    {
      "find": "\\\\ln\\(",
      "replace": "h(",
      "type": "regex",
      "description": "Natural log function: \\ln(x) -> h(x)",
      "priority": 6
    },
    {
      "find": "ln\\(",
      "replace": "h(",
      "type": "regex",
      "description": "Natural log function no backslash: ln(x) -> h(x)",
      "priority": 6
    },
    {
      "find": "\\}",
      "replace": ")",
      "type": "regex",
      "description": "Close brace to parenthesis: } -> )",
      "priority": 7
    },
    {
      "find": "\\{",
      "replace": "(",
      "type": "regex",
      "description": "Open brace to parenthesis: { -> (",
      "priority": 7
    },
    {
      "find": "\\^",
      "replace": "^",
      "type": "regex",
      "description": "Power operator: \\^ -> ^",
      "priority": 8
    },
    {
      "find": "_",
      "replace": "_",
      "type": "regex",
      "description": "Subscript operator: _ -> _",
      "priority": 9
    }
  ],
  "keylog_encoding_rules": {
    "fx799": {
      "coordinates": {
        "point_separator": "=",
        "coordinate_separator": "1",
        "prefix": "wj",
        "suffix": "qT"
      },
      "operations": {
        "distance": {
          "prefix": "T1",
          "separator": "C",
          "suffix": "RT2="
        },
        "intersection": {
          "prefix": "R1",
          "separator": "T",
          "suffix": "ST3="
        }
      },
      "calculator_mode": "w9",
      "decimal_separator": "."
    },
    "fx880": {
      "coordinates": {
        "point_separator": "=",
        "coordinate_separator": "1",
        "prefix": "wR$$||j",
        "suffix": "qT"
      },
      "operations": {
        "distance": {
          "prefix": "T1",
          "separator": "C",
          "suffix": "RT2="
        },
        "intersection": {
          "prefix": "R1",
          "separator": "T",
          "suffix": "ST3="
        }
      },
      "calculator_mode": "wR$$||9",
      "decimal_separator": ".",
      "bluetooth_enabled": true
    }
  },
  "special_characters": {
    "space": "",
    "comma": "1",
    "dot": ".",
    "minus": "p",
    "plus": "+",
    "equals": "=",
    "left_paren": "(",
    "right_paren": ")"
  },
  "metadata": {
    "version": "1.0",
    "description": "Quy tắc mã hóa LaTeX và keylog cho Geometry Mode",
    "created_for": "Geometry Calculator API",
    "last_updated": "2025-11-05"
  }
}
